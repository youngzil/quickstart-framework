<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

  <modelVersion>4.0.0</modelVersion>
  <groupId>org.quickstart</groupId>
  <artifactId>quickstart-framework</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <packaging>pom</packaging>
  <name>${project.artifactId}-${project.version}</name>
  <url>http://maven.apache.org</url>
  <description>Demo project for XXX</description>

  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <JAVA_HOME>/Library/Java/JavaVirtualMachines/jdk1.8.0_281.jdk/Contents/Home</JAVA_HOME>
  </properties>

  <dependencies>

    <dependency>
      <groupId>org.projectlombok</groupId>
      <artifactId>lombok</artifactId>
      <version>1.18.16</version>
      <scope>provided</scope>
    </dependency>

    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.13.1</version>
      <scope>test</scope>
    </dependency>

  </dependencies>

  <modules>
    <module>quickstart-asm</module>
    <module>quickstart-code</module>
    <module>quickstart-design-pattern</module>
    <module>quickstart-disruptor</module>
    <module>quickstart-eaio-uuid</module>
    <module>quickstart-elastic-job</module>
    <module>quickstart-example</module>
    <module>quickstart-fastdfs</module>
    <module>quickstart-groovy</module>
    <module>quickstart-guava</module>
    <module>quickstart-guice</module>
    <module>quickstart-javase</module>
    <module>quickstart-jfinal</module>
    <module>quickstart-json</module>
    <module>quickstart-jvm-serializer</module>
    <module>quickstart-kotlin</module>
    <module>quickstart-linux</module>
    <module>quickstart-logging</module>
    <module>quickstart-lombok</module>
    <module>quickstart-machine-learning</module>
    <module>quickstart-mnemonic</module>
    <module>quickstart-objenesis</module>
    <module>quickstart-proguard</module>
    <module>quickstart-proxy</module>
    <module>quickstart-quartz</module>
    <module>quickstart-scala</module>
    <module>quickstart-servicecomb</module>
    <module>quickstart-spring-integration</module>
    <module>quickstart-spring-integration-kafka</module>
    <module>quickstart-spring-security</module>
    <module>quickstart-web</module>
    <module>quickstart-xbean</module>
    <module>quickstart-xml</module>
    <module>quickstart-xmlbeans</module>
    <module>quickstart-yaml</module>
    <module>quickstart-websocket</module>
    <module>quickstart-dropwizard</module>
    <module>quickstart-crypto</module>
    <module>quickstart-jool</module>
    <module>quickstart-joou</module>
    <module>quickstart-joor</module>
    <module>quickstart-joox</module>
    <module>quickstart-reflect</module>
    <module>quickstart-jython</module>
    <module>quickstart-jgit</module>
    <module>quickstart-apache-commons</module>
    <module>quickstart-velocity</module>
    <module>quickstart-servlet</module>
    <module>quickstart-plexus</module>
    <module>quickstart-jctools</module>
    <module>quickstart-hppc</module>
    <module>quickstart-lua</module>
    <module>quickstart-easyexcel</module>
    <module>quickstart-hutool</module>
    <module>quickstart-jodd</module>
    <module>quickstart-jol</module>
    <module>quickstart-sensitive</module>
    <module>quickstart-liteflow</module>
    <module>quickstart-simpleimage</module>
    <module>quickstart-oshi</module>
    <module>quickstart-metrics</module>
    <module>quickstart-compress</module>
  </modules>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.8.1</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
          <compilerArgument>-parameters</compilerArgument>
        </configuration>
      </plugin>
    </plugins>
  </build>

  <repositories>

    <repository>
      <id>alimaven-public</id>
      <name>aliyun maven</name>
      <url>https://maven.aliyun.com/repository/public</url>
      <releases>
        <enabled>true</enabled>
        <updatePolicy>always</updatePolicy>
      </releases>
      <snapshots>
        <enabled>true</enabled>
        <updatePolicy>always</updatePolicy>
        <checksumPolicy>fail</checksumPolicy>
      </snapshots>
    </repository>

    <repository>
      <id>repo1-public</id>
      <name>repo1 maven</name>
      <url>https://repo1.maven.org/maven2/</url>
      <releases>
        <enabled>true</enabled>
        <updatePolicy>always</updatePolicy>
      </releases>
      <snapshots>
        <enabled>true</enabled>
        <updatePolicy>always</updatePolicy>
        <checksumPolicy>fail</checksumPolicy>
      </snapshots>
    </repository>

  </repositories>

</project>